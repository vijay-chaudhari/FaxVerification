@page
@using FaxVerification.Permissions;
@using FaxVerification.Web.Pages.OCR
@using Microsoft.AspNetCore.Authorization;
@model IndexModel
@inject IAuthorizationService AuthorizationService
@section styles{
    <abp-style src="/Pages/index.css" />
}
@section scripts {
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdf.js/2.10.377/pdf.min.js"></script>
    <script src="/libs/tiff/tiff.min.js"></script>
    <script src="/libs/pdfannotate/pdfAnnotate.js"></script>
    <script src='https://unpkg.com/tesseract.js@4.0.2/dist/tesseract.min.js'></script>
    <abp-script src="/Pages/OCR/Index.js" />
    <abp-script src="/Pages/OCR/EditModal.js" />
}

<abp-card>
    <abp-card-header>
        <abp-row>
            <abp-column size="_8">
                <abp-card-title>Processed Documents</abp-card-title>
            </abp-column>
        </abp-row>  
    </abp-card-header>
    <abp-card-body>
        <abp-table striped-rows="true" id="BooksTable"></abp-table>
    </abp-card-body>
</abp-card>